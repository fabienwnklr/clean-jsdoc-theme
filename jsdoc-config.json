{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["../isigeo_v60/javascript/isigeoGUI.js", "../isigeo_v60/types/isigeo.d.js", "../isigeo_v60/types/layoutManager.d.js"]
    },
    "plugins": ["plugins/markdown", "node_modules/jsdoc-tsimport-plugin/index.js", "./static/scripts/custom-tags.js"],
    "opts": {
        "encoding": "utf8",
        "readme": "README.md",
        "destination": "output/v4",
        "recurse": true,
        "template": "./",
        "tutorials": "../isigeo_v60/tutorials",
        "theme_opts": {
            "include_js": ["./static/scripts/typedef.js"],
            "search": true,
            "homepageTitle": "Clean JSDoc Theme",
            "default_theme": "light",
            "displayModuleHeader": true,
            "title": "clean-jsdoc-theme",
            "footer": "<div style='margin-bottom: 0.5rem;'>clean-jsdoc-theme</div> Fork: <a href='https://github.com/ankitskvmdam/clean-jsdoc-theme'>https://github.com/ankitskvmdam/clean-jsdoc-theme</a>",
            "meta": [
                {
                    "name": "Author",
                    "content": "Ankit Kumar"
                },
                {
                    "name": "Description",
                    "content": "A beautifully crafted theme for jsdoc"
                }
            ]
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}
